<jstranslator>
    <porterbuddy-location-save-success translate="message" module="convert_porterbuddy">
        <message>Thank you! Delivery location has been updated.</message>
    </porterbuddy-location-save-success>
    <porterbuddy-location-save-error translate="message" module="convert_porterbuddy">
        <message>An error occurred when saving delivery location.</message>
    </porterbuddy-location-save-error>
    <porterbuddy-geocoding-error translate="message" module="convert_porterbuddy">
        <message>We could not automatically find your location on the map. Please place a marker.</message>
    </porterbuddy-geocoding-error>
    <porterbuddy-set-location-first translate="message" module="convert_porterbuddy">
        <message>Please set location first.</message>
    </porterbuddy-set-location-first>
    <porterbuddy-previous-day translate="message" module="convert_porterbuddy">
        <message>Previous Day</message>
    </porterbuddy-previous-day>
    <porterbuddy-today translate="message" module="convert_porterbuddy">
        <message>Today</message>
    </porterbuddy-today>
</jstranslator>
